    "@tanstack/router-generator": "^1.74.2",
    "@types/react": "^18.3.12",
    "@types/react-dom": "^18.3.1",
    "@types/yargs-parser": "^21.0.3",
    "@umijs/mako": "^0.9.3",
    "react": "19.0.0-rc-02c0e824-20241028",
