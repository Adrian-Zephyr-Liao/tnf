{
  "name": "@umijs/tnf",
  "version": "0.0.0-alpha.2",
  "scripts": {
    "build": "father build",
    "check": "prettier --check .",
    "format": "prettier --write .",
    "test": "vitest",
    "prepare": "lint-staged && npm run check && father doctor",
    "release": "tsx scripts/release.ts"
  },
  "bin": {
    "tnf": "bin/tnf.js"
  },
  "files": [
    "bin",
    "client",
    "dist"
  ],
  "devDependencies": {
    "@total-typescript/tsconfig": "^1.0.4",
    "father": "^4.5.1",
    "git-repo-info": "^2.1.1",
    "husky": "^9.1.6",
    "lint-staged": "^15.2.10",
    "prettier": "^3.3.3",
    "tsx": "^4.19.2",
    "typescript": "^5.6.3",
    "vitest": "^2.1.3",
    "zx": "^8.1.9"
  },
  "lint-staged": {
    "*.{jsx,less,md,json}": [
      "prettier --no-error-on-unmatched-pattern --cache --write"
    ],
    "*.ts?(x)": [
      "prettier --no-error-on-unmatched-pattern --cache --parser=typescript --write"
    ]
  },
  "publishConfig": {
    "access": "public"
  }
}
